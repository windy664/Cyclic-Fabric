{
	"schemaVersion": 1,
	"id": "cyclic",
	"version": "${version}",
	"name": "Cyclic Fabric",
	"description": "A Fabric port of Cyclic by KnownSH",
	"authors": [
		"KnownSH",
		"Lothrazar [Forge]"
	],
	"contact": {
		"sources": "https://github.com/KnownSH/Cyclic-Fabric",
		"issues": "https://github.com/KnownSH/Cyclic-Fabric/issues"
	},
	"license": "MIT",
	"icon": "assets/cyclic/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.lothrazar.cyclic.Cyclic"
		],
		"client": [
			"com.lothrazar.cyclic.client.CyclicClient"
		],
		"fabric-datagen": [
			"com.lothrazar.cyclic.datagen.CyclicDataGenerator"
		],
		"jei_mod_plugin": [
			"com.lothrazar.cyclic.compat.jei.CyclicPluginJEI"
		],
		"rei_client": [
			"com.lothrazar.cyclic.compat.rei.CyclicPluginREI"
		],
		"emi": [
			"com.lothrazar.cyclic.compat.emi.CyclicPluginEMI"
		]
	},
	"mixins": [
		"cyclic.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
